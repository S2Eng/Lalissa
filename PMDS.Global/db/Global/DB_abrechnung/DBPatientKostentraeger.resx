  <data name="oleDbCommand6.CommandText" xml:space="preserve">
    <value>SELECT DISTINCT 
                      Kostentraeger.ID, Kostentraeger.Name, Kostentraeger.Strasse, Kostentraeger.PLZ, Kostentraeger.Ort, Kostentraeger.Rechnungsempfaenger, 
                      Kostentraeger.Rechnungsanschrift, Kostentraeger.BLZ, Kostentraeger.Kontonr, Kostentraeger.Bank, Kostentraeger.FIBUKonto, 
                      Kostentraeger.ErlagscheingebuehrJN, Kostentraeger.Betrag, Kostentraeger.TransferleistungJN, Kostentraeger.TaschengeldJN, Kostentraeger.Zahlart, 
                      Kostentraeger.PatientbezogenJN
FROM         Kostentraeger INNER JOIN
                      PatientKostentraeger ON Kostentraeger.ID = PatientKostentraeger.IDKostentraeger
WHERE     (PatientKostentraeger.AbgerechnetBis IS NOT NULL) AND (PatientKostentraeger.GueltigAb &lt;= ?) AND (PatientKostentraeger.GueltigBis &gt;= ?) AND 
                      (PatientKostentraeger.GueltigBis &lt;= ?) AND (Kostentraeger.PatientbezogenJN = 0) OR
                      (PatientKostentraeger.AbgerechnetBis IS NOT NULL) AND (PatientKostentraeger.GueltigAb &gt;= ?) AND (PatientKostentraeger.GueltigBis &gt;= ?) AND 
                      (PatientKostentraeger.GueltigBis &lt;= ?) AND (Kostentraeger.PatientbezogenJN = 0) OR
                      (PatientKostentraeger.AbgerechnetBis IS NOT NULL) AND (PatientKostentraeger.GueltigAb &gt;= ?) AND (PatientKostentraeger.GueltigBis &gt;= ?) AND 
                      (Kostentraeger.PatientbezogenJN = 0) AND (PatientKostentraeger.GueltigAb &lt;= ?) OR
                      (PatientKostentraeger.AbgerechnetBis IS NOT NULL) AND (PatientKostentraeger.GueltigAb &lt;= ?) AND (PatientKostentraeger.GueltigBis &gt;= ?) AND 
