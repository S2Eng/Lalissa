  <data name="oleDbCommand7.CommandText" xml:space="preserve">
    <value>SELECT        Patient.ID, Patient.Nachname + '  ' + Patient.Vorname AS Name, Patient.Geburtsdatum, Abteilung.Bezeichnung AS Abteilung, Bereich.Bezeichnung AS Bereich, 
                         UrlaubVerlauf.Text AS UrlaubText, Abteilung.ID AS IDAbteilung, Aufenthalt.IDBereich, Aufenthalt.IDKlinik, Aufenthalt.ID AS IDAufenthalt
FROM            UrlaubVerlauf RIGHT OUTER JOIN
                         Aufenthalt ON UrlaubVerlauf.ID = Aufenthalt.IDUrlaub LEFT OUTER JOIN
                         AufenthaltVerlauf INNER JOIN
                         Abteilung ON AufenthaltVerlauf.IDAbteilung_Nach = Abteilung.ID LEFT OUTER JOIN
                         Bereich ON AufenthaltVerlauf.IDBereich = Bereich.ID ON Aufenthalt.IDAufenthaltVerlauf = AufenthaltVerlauf.ID RIGHT OUTER JOIN
                         Patient ON Aufenthalt.IDPatient = Patient.ID INNER JOIN
                         PatientKostentraeger ON Patient.ID = PatientKostentraeger.IDPatient INNER JOIN
                         Kostentraeger ON Kostentraeger.ID = PatientKostentraeger.IDKostentraeger
WHERE        (Kostentraeger.ID = ?) AND (Aufenthalt.Entlassungszeitpunkt IS NULL)
ORDER BY Name</value>
  </data>
