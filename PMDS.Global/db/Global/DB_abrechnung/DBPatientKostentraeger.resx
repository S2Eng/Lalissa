  <data name="oleDbCommand11.CommandText" xml:space="preserve">
    <value>SELECT        TOP (100) PERCENT Patient.ID, Patient.Nachname + '  ' + Patient.Vorname AS Name, Patient.Geburtsdatum, Abteilung.Bezeichnung AS Abteilung, 
                         Bereich.Bezeichnung AS Bereich, UrlaubVerlauf.Text AS UrlaubText, Abteilung.ID AS IDAbteilung, Aufenthalt.IDBereich, Aufenthalt.IDKlinik, 
                         Aufenthalt.ID AS IDAufenthalt
FROM            UrlaubVerlauf RIGHT OUTER JOIN
                         Aufenthalt ON UrlaubVerlauf.ID = Aufenthalt.IDUrlaub LEFT OUTER JOIN
                         AufenthaltVerlauf INNER JOIN
                         Abteilung ON AufenthaltVerlauf.IDAbteilung_Nach = Abteilung.ID LEFT OUTER JOIN
                         Bereich ON AufenthaltVerlauf.IDBereich = Bereich.ID ON AufenthaltVerlauf.ID = Aufenthalt.IDAufenthaltVerlauf RIGHT OUTER JOIN
                         Patient ON Patient.ID = Aufenthalt.IDPatient
WHERE        (NOT (Aufenthalt.ID IS NULL)) AND (Aufenthalt.Entlassungszeitpunkt IS NULL) AND
                             ((SELECT        COUNT(*) AS anz
                                 FROM            PatientKostentraeger AS k2 INNER JOIN
                                                          Kostentraeger AS Kostentraeger_1 ON k2.IDKostentraeger = Kostentraeger_1.ID
                                 WHERE        (k2.IDPatient = Patient.ID) AND (Kostentraeger_1.TransferleistungJN = 0) AND (Kostentraeger_1.PatientbezogenJN = 0)) = 0)
GROUP BY Patient.ID, Patient.Nachname, Patient.Vorname, Patient.Geburtsdatum, Abteilung.Bezeichnung, Bereich.Bezeichnung, UrlaubVerlauf.Text, Abteilung.ID, 
                         Aufenthalt.IDBereich, Aufenthalt.IDKlinik, Aufenthalt.ID
ORDER BY Name</value>
  </data>
